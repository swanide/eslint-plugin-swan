        "@types/jest": "^27.0.3",
        "@types/node": "^14.14.14",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "import-fresh": "^3.3.0",
        "jest": "^27.4.5",
        "ts-jest": "^27.1.2",
        "typescript": "^4.5.4"
