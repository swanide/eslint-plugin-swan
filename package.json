{
    "name": "eslint-plugin-swan",
    "version": "0.0.1-beta.0",
    "description": "swan eslint plugin",
    "main": "lib/index.js",
    "bin": {
        "swan-lint": "bin/swan-lint"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "prepublish": "npm run build",
        "build": "tsc",
